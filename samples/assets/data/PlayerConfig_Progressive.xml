<?xml version="1.0" encoding="UTF-8"?>
<player 
	width="640" height="480" 
	scaleMode="letterbox" playbackType="PROGRESSIVE" 
	isLive="false" autoPlay="true" updateInterval="250"
	hasCaptions="false">
	<playlist>
		<mediaElement>
			<id>faery</id>
			<mimeType>video/x-flv</mimeType>
			<streamType>recorded</streamType>
			<deliveryType>progressive</deliveryType>
			<media url="http://mediapm.edgesuite.net/strobe/content/test/AFaerysTale_sylviaApostol_640_500_short.flv" />
		</mediaElement>
	</playlist>
	<skin path="assets/skins/Lunar.swf">
		<skinElement id="controlBar"
			elementClass="com.realeyes.osmfplayer.controls.ControlBar" 
			initMethod="initControlBarInstance" 
			scaleMode="NONE" 
			hAdjust="0" vAdjust="0" 
			vAlign="BOTTOM"
			autoPosition="true"
			draggable="true" 
			autoHide="true" />
		<skinElement id="loadingIndicator"
			elementClass="com.realeyes.osmfplayer.controls.LoadingIndicator" 
			initMethod="initLoadingIndicatorInstance" 
			hAdjust="0" vAdjust="0"
			vAlign="CENTER"
			autoPosition="true" />
	</skin>
</player>